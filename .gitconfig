[user]
  name = Sumanyu Sharma
  email = sumanyu@hotmail.com
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[diff]
  tool = opendiff
[difftool]
  prompt = false
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
